{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "default-param-last": "off",
        "no-underscore-dangle": "off",
        "no-restricted-syntax": [
            "error", 
            "LabeledStatement", 
            "WithStatement"]
    }
}
